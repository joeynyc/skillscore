{
  "name": "skillscore",
  "version": "1.2.0",
  "description": "A CLI tool that evaluates AI agent skills and produces quality scores",
  "main": "dist/index.js",
  "bin": {
    "skillscore": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "lint": "eslint src/**/*.ts tests/**/*.ts",
    "lint:fix": "eslint src/**/*.ts tests/**/*.ts --fix",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "ai",
    "agent",
    "skills",
    "evaluation",
    "scoring",
    "openclaw",
    "cli"
  ],
  "author": "Joey Rodriguez <info@socialwithjoey.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joeynyc/skillscore.git"
  },
  "bugs": {
    "url": "https://github.com/joeynyc/skillscore/issues"
  },
  "homepage": "https://github.com/joeynyc/skillscore#readme",
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "fs-extra": "^11.2.0",
    "glob": "^10.3.10",
    "yaml": "^2.3.4"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.10.5",
    "@typescript-eslint/eslint-plugin": "^8.55.0",
    "@typescript-eslint/parser": "^8.55.0",
    "@vitest/ui": "^4.0.18",
    "eslint": "^9.39.2",
    "eslint-plugin-node": "^11.1.0",
    "globals": "^17.3.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vitest": "^4.0.18"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
